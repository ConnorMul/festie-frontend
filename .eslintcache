     <div className=\"menu-container\">\n            <Link onClick={onClick} className=\"menu-trigger\">\n                {currentUser.username}\n\n            </Link>\n            <nav ref={dropdownRef} className={`menu ${isActive ? 'active' : 'inactive'}`}>\n                <ul>\n                    <li><Link to=\"/profile/reviews\" onClick={closeMenu}>Reviews</Link></li>\n                    <li><Link to=\"/profile/favorites\" onClick={closeMenu}>Favorites</Link></li>\n                    <li><Link to=\"/profile\" onClick={closeMenu}>Edit your Profile</Link></li>\n                    <li><Link to=\"/\" onClick={closeMenu, handleLogout} >Logout</Link></li>\n                </ul>\n            </nav>\n      </div>\n    );\n};\n\nexport default DropdownProfile",["152","153"],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/Profile.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/Trending.js",["154"],"import React, { useEffect } from 'react'\nimport './styles/Trending.css'\nimport TrendingFestivalCard from './TrendingFestivalCard'\n\nfunction Trending({ currentUser, favorites, setFavorites, trending, setTrending, setFavoritesLength }) {\n\n    useEffect(() => {\n        fetch(`${process.env.REACT_APP_API_BASE_URL}/festivals`)\n        .then(r => r.json())\n        .then(festivalObjs => {\n            setTrending(festivalObjs.filter(festival => festival.average_star_rating > 0))\n        })\n    },[])\n\n\n\n    const sortedFestivals = trending.sort((fest1, fest2) => fest2.average_star_rating - fest1.average_star_rating).slice(0, 8)\n    .map(festival => {\n        const favorite = favorites.find(fav => fav.festival_id === festival.id)\n        return <TrendingFestivalCard key={festival.id} festival={festival} currentUser={currentUser} favorite={favorite} favorites={favorites} setFavorites={setFavorites} setFavoritesLength={setFavoritesLength}/>\n    })\n\n    console.log(sortedFestivals)\n    return (\n        <div className=\"trending-container\">\n            <h1 className=\"trending-title\">Talk about a killer set!</h1>\n            <h1 className=\"trending-title\">These fests are top rated among our users</h1>\n            {sortedFestivals}\n        </div>\n    )\n}\n\nexport default Trending","/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/FestFriend.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/EditProfileForm.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/PostCard.js",["155","156"],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/Signup.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/FestieFinder.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/FestieCard.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/FestieProfile.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/FestiePostCard.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/TrendingFestivalCard.js",[],"/Users/ConnorMulholland_1/Development/code/Mod5/festie/festie-frontend/src/components/ScrollToTop.js",[],{"ruleId":"157","severity":1,"message":"158","line":1,"column":17,"nodeType":"159","messageId":"160","endLine":1,"endColumn":25},{"ruleId":"161","replacedBy":"162"},{"ruleId":"163","replacedBy":"164"},{"ruleId":"161","replacedBy":"165"},{"ruleId":"163","replacedBy":"166"},{"ruleId":"161","replacedBy":"167"},{"ruleId":"163","replacedBy":"168"},{"ruleId":"157","severity":1,"message":"169","line":10,"column":12,"nodeType":"159","messageId":"160","endLine":10,"endColumn":17},{"ruleId":"170","severity":1,"message":"171","line":26,"column":12,"nodeType":"172","endLine":26,"endColumn":23,"suggestions":